//POTD 12-01-24 
int a=0,b=0,j=s.size()-1;
for(int i=0;i<s.size()/2;i++){
    if(s[i]=='a' or s[i]=='e' or s[i]=='i' or s[i]=='o' or s[i]=='u' or s[i]=='A' or s[i]=='E' or s[i]=='I' or s[i]=='O' or s[i]=='U') a++;
    if(s[j]=='a' or s[j]=='e' or s[j]=='i' or s[j]=='o' or s[j]=='u' or s[j]=='A' or s[j]=='E' or s[j]=='I' or s[j]=='O' or s[j]=='U') b++;
    j--;
}
if(a==b) return true;
return false;
